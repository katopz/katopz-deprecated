package com.squidder.base.baseLoader.events {	import flash.events.Event;			/**	 * @author Jon Reiling	 */	public class BaseLoaderProgressEvent extends Event {		public static const PROGRESS : String = "BaseLoaderProgress";		public var percentLoaded : Number;		public function BaseLoaderProgressEvent(type : String, bubbles : Boolean = false, cancelable : Boolean = false) {			super( type , bubbles , cancelable );		}				override public function clone() : Event {						var ev : BaseLoaderProgressEvent = new BaseLoaderProgressEvent( type , bubbles , cancelable );			ev.percentLoaded = percentLoaded;						return ev;		}	}}