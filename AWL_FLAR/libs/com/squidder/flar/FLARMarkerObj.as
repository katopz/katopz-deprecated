package com.squidder.flar {	/**	 * @author Jon Reiling	 */	public class FLARMarkerObj {				private var _markerSrc:String;		private var _markerSegements:int;		private var _markerSize:int;		private var _markerWidth:int;				public function FLARMarkerObj( markerSrc:String , markerSegements:int , markerSize:int , markerWidth:int ) {						_markerSrc = markerSrc;			_markerSegements = markerSegements;			_markerSize = markerSize;			_markerWidth = markerWidth;					}				public function get markerSrc() : String {						return _markerSrc;			}		public function get markerSegements() : int {						return _markerSegements;			}		public function get markerSize() : int {						return _markerSize;			}		public function get markerWidth() : int {						return _markerWidth;			}	}}