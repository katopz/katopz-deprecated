package com.squidder.base.baseLoader.types {	import flash.net.URLLoader;	import flash.net.URLLoaderDataFormat;		import com.squidder.base.baseLoader.BaseLoaderProperties;		/**	 * @author Jon Reiling	 */	public class BinaryTextItem extends TextItem {		/**		 * Similar to a TextItem, but loaded in with a dataFormat of BINARY.		 * @param url URL of the item.		 * @param props Properties, if any, of the item.		 */		 public function BinaryTextItem(url : String, props : BaseLoaderProperties) {			super( url , props );						_urlLoader.dataFormat = URLLoaderDataFormat.BINARY;		}	}}