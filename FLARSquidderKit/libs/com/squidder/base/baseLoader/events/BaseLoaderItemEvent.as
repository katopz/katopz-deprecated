package com.squidder.base.baseLoader.events {	import flash.events.Event;		import com.squidder.base.baseLoader.types.BaseLoaderItem;			/**	 * @author Jon Reiling	 */	public class BaseLoaderItemEvent extends Event {		public static const ITEM_COMPLETE : String = "BaseLoaderItemComplete";		public static const ITEM_ERROR : String = "BaseLoaderItemError";		public var loaderItem : BaseLoaderItem;		public function BaseLoaderItemEvent(type : String, bubbles : Boolean = false, cancelable : Boolean = false) {			super( type , bubbles , cancelable );		}				override public function clone() : Event {						var ev : BaseLoaderItemEvent = new BaseLoaderItemEvent( type , bubbles , cancelable );			ev.loaderItem = loaderItem;						return ev;		}			}}