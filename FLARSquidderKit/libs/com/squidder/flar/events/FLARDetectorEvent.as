package com.squidder.flar.events {	import flash.events.Event;		/**	 * @author Jon Reiling	 */	public class FLARDetectorEvent extends Event {				public static const MARKER_ADDED : String = "onMarkerAdded";		public static const MARKER_REMOVED : String = "onMarkerRemoved";				public var codeId:int;		public var codeIndex:int;		public function FLARDetectorEvent(type : String, bubbles : Boolean = false, cancelable : Boolean = false) {			super( type , bubbles , cancelable );		}				override public function clone() : Event {					var ev : FLARDetectorEvent = new FLARDetectorEvent( type , bubbles , cancelable );			ev.codeId = codeId;			ev.codeIndex = codeIndex;							return ev;		}	}}